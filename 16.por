programa {
  funcao inicio() {
    real a, b

    escreva("Renda mensal: ")
    leia (a)

    se(a<=2000){
      escreva("Empréstimo Indisponível.")}
      senao{
      escreva("SPC (1-Boa / 2-Má):")
      leia (b)
      }
      se(b==1){
        escreva("Empréstimo Disponível.")
      }
      senao se(b==2){
        escreva("Empréstimo Indisponível")
      }
      }
      }
    }
  }
}
